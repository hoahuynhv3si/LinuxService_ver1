{  
    "Serilog": {  
      "MinimumLevel": "Debug",  
      "WriteTo": [  
        {  
          "Name": "RollingFile",  
          "Args": {  
            "logDirectory": ".\\Logs",  
            "fileSizeLimitBytes": 1024,  
            "pathFormat": "Logs/myservice.{Date}.log",  
            "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"  
          }  
        }  
      ]  
    },
    "Redis":{
      "Host" : "localhost",
      "Port" : 6379
    }
  }  